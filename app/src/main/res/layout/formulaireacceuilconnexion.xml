<?xml version="1.0" encoding="utf-8"?>
<!-- logo -->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/formulaire_"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/formulaire_">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="71dp"
        android:background="@drawable/logo" />
    <!-- TitreApplication -->
    <!--
    Font family: Inter
    Line height: 51sp
    translationY 3.81sp compensates for the line height adjustment of text
-->
    <TextView
        android:id="@+id/titreapplic"
        android:layout_width="92dp"
        android:layout_height="102dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="159dp"
        android:layout_marginTop="106dp"
        android:fontFamily="sans-serif"
        android:gravity="top"
        android:includeFontPadding="true"
        android:lineHeight="51sp"
        android:lineSpacingExtra="8sp"
        android:text="@string/titreapplic"
        android:textAlignment="center"
        android:textAppearance="@style/titreapplic"
        android:textSize="36sp"
        android:translationY="3.81sp" />
    <!-- ChoixCategorie -->
    <EditText
        android:id="@+id/choixcatego"
        android:layout_width="166.11dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="208dp"
        android:layout_marginTop="260dp"
        android:background="#00000000" />

    <!-- drop -->
    <View
        android:id="@+id/drop"
        android:layout_width="36.64dp"
        android:layout_height="33.34dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="323dp"
        android:layout_marginTop="271dp"
        android:background="#00000000" />

    <!-- rectangle1cnx -->
    <View
        android:layout_width="56.41dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="220.54dp"
        android:layout_marginTop="253dp"
        android:background="@drawable/rectangle1cnx"
        android:clipToOutline="true" />

    <!-- Profil -->
    <TextView
        android:id="@+id/profil"
        android:layout_width="36dp"
        android:layout_height="14dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="229dp"
        android:layout_marginTop="254dp"
        android:fontFamily="sans-serif"
        android:gravity="center_horizontal|top"
        android:lineHeight="14sp"
        android:lineSpacingExtra="2sp"
        android:text="@string/profil"
        android:textAlignment="center"
        android:textAppearance="@style/profil"
        android:textSize="10ps"
        android:translationY="1sp" />

    <!-- emailUtilisateur -->
    <EditText
        android:id="@+id/emailutilis"
        android:layout_width="341dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="332dp"
        android:background="#00000000"
        />

    <View
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/titreEmail"
        android:layout_width="99.59dp"
        android:layout_height="16dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="47.07dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="416dp"
        android:clipToOutline="true"
        android:background="@drawable/petittitre"
        />
